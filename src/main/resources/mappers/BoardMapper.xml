<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.boilerPlate.app.Mapper.BoardMapper">
        
    <select id="selectBolardList" parameterType="com.boilerPlate.app.Vo.Board" resultType="com.boilerPlate.app.Vo.Board">
        select 
        	id
        	,name
        	,content
        	,sumnail
        	,credate
        	,uptdate
        from board
    </select>
    
</mapper>
